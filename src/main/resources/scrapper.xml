<?xml version="1.0" encoding="UTF-8"?>
<root>
    <scrapperConfigList>
        <scrapperConfig baseUrl="https://tvshows88.org">
            <patternConfig>
                <patternDefinition>/episode/${serieName}/-season-/${seasonNumber}/-episode-/${episodeNumber}</patternDefinition>
                <patternBindings>
                    <bind patternVariable="seasonNumber" numberFormat="%1d" />
                    <bind patternVariable="episodeNumber" numberFormat="%1d" />
                </patternBindings>
            </patternConfig>
        </scrapperConfig>

